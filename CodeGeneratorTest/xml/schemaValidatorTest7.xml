<?xml version="1.0" encoding="utf-8" ?> 
<!-- test duplicate tables in different namespaces -->
<application 
	name="schemaloadertest1" revised="8/3/2006"
	xmlns="codegenschema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="myschema file:schema.xsd">

	<namespace>
		<name>Namespace1</name>						
		<class>
			<name>Class1</name>
			<type>record</type>					
			<table>
				<name>Table1</name>	
				<field>	
					<name>Field1</name>			
					<sqldatatype>int</sqldatatype>							
				</field>				
			</table>
		</class>		
	</namespace>	
	
	<namespace>
		<name>Namespace2</name>		
		<class>
			<name>Class1</name>
			<type>record</type>		
			<table>
				<name>Table1</name>		
				<field>	
					<name>Field1</name>			
					<sqldatatype>int</sqldatatype>							
				</field>
			</table>			
		</class>				
	</namespace>
	
</application>
