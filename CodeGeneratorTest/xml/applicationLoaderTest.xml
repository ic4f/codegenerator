				</field>
				<field>
					<name>A</name>
					<sqldatatype>int</sqldatatype>
					<excludefromtable>true</excludefromtable>
				</field>		
				<additionalfield>
					<name>B</name>
					<sqldatatype>int</sqldatatype>
